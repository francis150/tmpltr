- /wp:columns -->\n\n</div>\n<!-- /wp:group -->\n\n<!-- wp:group {\"style\":{\"color\":{\"background\":\"#fafbfc\"},\"spacing\":{\"padding\":{\"top\":\"70px\",\"bottom\":\"70px\",\"left\":\"40px\",\"right\":\"40px\"}},\"border\":{\"top\":{\"color\":\"#f3f4f6\",\"width\":\"1px\"},\"bottom\":{\"color\":\"#f3f4f6\",\"width\":\"1px\"}}},\"layout\":{\"type\":\"constrained\",\"contentSize\":\"960px\"}} -->\n<div class=\"wp-block-group has-background\" style=\"border-top-color:#f3f4f6;border-top-width:1px;border-bottom-color:#f3f4f6;border-bottom-width:1px;background-color:#fafbfc;padding-top:70px;padding-right:40px;padding-bottom:70px;padding-left:40px\">\n\n<!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"fontSize\":\"26px\",\"fontWeight\":\"600\",\"lineHeight\":\"1.3\"},\"color\":{\"text\":\"#111827\"},\"spacing\":{\"margin\":{\"bottom\":\"32px\"}}}} -->\n<h2 class=\"wp-block-heading has-text-align-center has-text-color\" style=\"color:#111827;font-size:26px;font-weight:600;line-height:1.3;margin-bottom:32px\">{faq_heading}</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"16px\",\"lineHeight\":\"1.9\"},\"color\":{\"text\":\"#374151\"}}} -->\n<p style=\"color:#374151;font-size:16px;line-height:1.9\">{faq_content}</p>\n<!-- /wp:paragraph -->\n\n</div>\n<!-- /wp:group -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"60px\",\"bottom\":\"60px\",\"left\":\"40px\",\"right\":\"40px\"}}},\"layout\":{\"type\":\"constrained\",\"contentSize\":\"700px\"}} -->\n<div class=\"wp-block-group\" style=\"padding-top:60px;padding-right:40px;padding-bottom:60px;padding-left:40px\">\n\n<!-- wp:separator {\"style\":{\"color\":{\"background\":\"#e5e7eb\"},\"spacing\":{\"margin\":{\"bottom\":\"48px\"}}},\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-alpha-channel-opacity has-background is-style-wide\" style=\"background-color:#e5e7eb;color:#e5e7eb;margin-bottom:48px\"/>\n<